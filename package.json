{
  "name": "electron-semantic-boilerplate",
  "version": "0.1.0",
  "description": "Start point for ElectronJS projects with ReactJS and SemanticUI.",
  "main": "./prod/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron dev/main.js --debug",
    "build-dev": "webpack --watch --mode development --config config/webpack.dev.config.js",
    "build-prod": "webpack --mode production --config config/webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unnamed-community/electron-semantic-boilerplate.git"
  },
